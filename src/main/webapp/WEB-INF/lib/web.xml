<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd" version="5.0">
  <display-name>ENI-Encheres</display-name>
  <welcome-file-list>
    <welcome-file>ServletAccueil</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>Page d'accueil pour se connecter</description>
    <servlet-name>Accueil</servlet-name>
    <jsp-file>/WEB-INF/jsp/Accueil.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Accueil</servlet-name>
    <url-pattern>/accueil.html</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description>Liste d'enchères</description>
    <servlet-name>Encheres</servlet-name>
    <jsp-file>/WEB-INF/jsp/ListeEncheres.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Encheres</servlet-name>
    <url-pattern>/encheres.html</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description>Créer son profil</description>
    <servlet-name>CreerCompte</servlet-name>
    <jsp-file>/WEB-INF/jsp/EnregistrementUtilisateur.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreerCompte</servlet-name>
    <url-pattern>/EnregistrementUtilisateur.html</url-pattern>
  </servlet-mapping>
  
  
</web-app>